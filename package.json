{
  "private": true,
  "name": "webryday-calendar",
  "version": "0.0.0",
  "description": "A web-based everyday calendar: https://www.simonegiertz.com/every-day-calendar",
  "homepage": "ssmeke.io/webryday",
  "bugs": {
    "url": "https://github.com/SalomonSmeke/webryday/issues",
    "email": "salomon@ssmeke.io"
  },
  "license": "MIT",
  "author": "Salomon Smeke Cohen <salomon@ssmeke.io> (https://ssmeke.io)",
  "repository": "github:SalomonSmeke/webryday",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3"
  },
  "browserslist": ">1% or last 2 versions and not dead and not IE > 0 and not op_mini all",
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
